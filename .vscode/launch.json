    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
            "stopOnEntry": false,
            // "args": ["${workspaceRoot}/typeScript/config/config.js", "--suite", "smoke"],
            "args": ["${workspaceRoot}/typeScript/config/config.js"],
            "cwd": "${workspaceRoot}",
            // "env": {
